.box{
  background-color: #f7f8fa;
  width: 100%;
  height: 100%;
  padding-bottom: 20rpx;
  /* position:fixed; */
}
.section{
  width: 100%;
  margin-top: .2rem;
}
.item:last-of-type{
  border-bottom: 1px solid #dad5d5;
}
.title{
  color: #8a8a8a;
  font-size: .8rem;
  margin-left: .2rem;
}
.item{
  font-size: 1.1rem;
  display: flex;
  height: 90rpx;
  width: 100%;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  padding: 5rpx;
  box-sizing: border-box;
  padding-right: 10rpx;
  border-top: 1px solid #dad5d5;
}
.item-detail{
  display: inline-flex;
}
.item-detail image{
  margin:0 10rpx;
}
.item image{
  width: 60rpx;
  height: 60rpx;
}
.bottom{
  display: flex;
  justify-content: center;
  color: #aeaaaa;
  margin-top: .5rem;
  margin-bottom: .5rem;
  font-size: .9rem;
}